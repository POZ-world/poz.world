- content_for :page_title do
  = t('errors.409.title')

- content_for :content do
  = t('errors.409.content')

- content_for :error_code do
  = 409



- content_for :static_image do
  = error_asset_url(t('errors.409.static_image')) unless t('errors.409.static_image').nil?
  = error_asset_url('images/errors/409-static.png') if t('errors.409.static_image').nil?

- content_for :dynamic_image do
  = error_asset_url(t('errors.409.dynamic_image')) unless t('errors.409.dynamic_image').nil?
  = error_asset_url('images/errors/409-dynamic.png') if t('errors.409.dynamic_image').nil?